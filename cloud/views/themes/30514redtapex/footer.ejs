    data.email = $('#email').val();
    

    var contact = new Contactmessage();
    contact.save(data, {
      success: function(){
        $('#success').css('opacity', 1);
        $('form')[0].reset();

      },
      error: function() {
        $('#error').css('opacity', 1);
      }
    }); 

  }); 
});
</script>



