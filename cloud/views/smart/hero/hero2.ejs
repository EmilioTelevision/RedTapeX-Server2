<div class="container-fluid">
	<div class="row">
		<div class="col-sm-12 col-md-12 col-lg-12 post-sc jumbotron">
	        <div class="col-sm-12 col-md-6 col-lg-5  col-lg-offset-1 sc-pic">
		        

<% if (typeof contentItem.get('img_array') != 'undefined') { %>
            <% contentItem.get('img_array').forEach(function(pic) { %>
             <img  width="100%" class="img-responsive" alt="Responsive image" src='<%= pic %>'>
            <% }) %>
<% } %>



		        </div>
		        <div class="col-sm-12 col-md-6 col-lg-5 col-lg-offset-1 sc-text">
		            <p><%= contentItem.get('smalltitle') %></p>
		            <h1><%= contentItem.get('headline') %> <a href="#">Shop></a> </h1>
		            <p class="no-margin"><a href="#"><%= contentItem.get('buttontxt') %></a></p>
	        </div> 
	    </div>
	</div>
</div>



 